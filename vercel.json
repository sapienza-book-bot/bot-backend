{
    "version": 2,
    "builds": [
        {
            "src": "dist/app.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "pipeline": {
        "generate": {
            "outputs": [
                "dist/**"
            ]
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ]
}